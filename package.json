{
  "name": "eye-test-utils",
  "version": "1.0.4",
  "description": "Validation & calculation utilities for optical prescriptions (glasses & lenses).",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "typescript": "^5.1.3",
    "tsup": "^8.0.0",
    "vitest": "^0.40.0"
  },
  "keywords": [
    "lens",
    "optics",
    "prescription",
    "validation",
    "typescript"
  ],
  "author": "Elhussin Taha",
  "license": "MIT"
}
